(defproblem problem domain 
(
	(DRON dron1)
	(TRANSPORTADOR transportador1)
	(LOCALIZACION deposito)
	(LOCALIZACION loc1)
	(LOCALIZACION loc2)
	(CONTENIDO comida)
	(CONTENIDO medicina)

	(limite-transportador transportador1 150)
	(capacidad-transportador transportador1 150)
	(transportador-tiene transportador1 comida 0)
	(transportador-tiene transportador1 medicina 0)

	(dron-en dron1 deposito)

	(transportador-en transportador1 deposito)

	(loc-total-necesita loc1 50)
	(loc-total-necesita loc2 100)
	(loc-necesita loc1 comida 16)
	(loc-necesita loc1 medicina 34)
	(loc-necesita loc2 comida 29)
	(loc-necesita loc2 medicina 71)

	(loc-tiene deposito comida 45)
	(loc-tiene deposito medicina 105)

	(dron-libre dron1)

)
(
	(enviar-todo)
)
)
