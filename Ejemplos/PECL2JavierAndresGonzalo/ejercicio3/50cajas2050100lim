(defproblem problem domain
(
	(DRON dron1)
	(TRANSPORTADOR transportador1)
	(TRANSPORTADOR transportador2)
	(TRANSPORTADOR transportador3)
	(LOCALIZACION deposito)
	(LOCALIZACION loc1)
	(CONTENIDO comida)
	(CONTENIDO medicina)

	(limite-transportador transportador1 20)
	(limite-transportador transportador2 50)
	(limite-transportador transportador3 100)
	(capacidad-transportador transportador1 20)
	(capacidad-transportador transportador2 50)
	(capacidad-transportador transportador3 100)
	(transportador-tiene transportador1 comida 0)
	(transportador-tiene transportador1 medicina 0)
	(transportador-tiene transportador2 comida 0)
	(transportador-tiene transportador2 medicina 0)
	(transportador-tiene transportador3 comida 0)
	(transportador-tiene transportador3 medicina 0)

	(dron-en dron1 deposito)

	(transportador-en transportador1 deposito)
	(transportador-en transportador2 deposito)
	(transportador-en transportador3 deposito)

	(loc-total-necesita loc1 50)
	(loc-necesita loc1 comida 23)
	(loc-necesita loc1 medicina 27)

	(loc-tiene deposito comida 23)
	(loc-tiene deposito medicina 27)

	(dron-libre dron1)

)
(
	(enviar-todo)
)
)
